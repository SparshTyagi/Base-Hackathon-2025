# ============ DEPLOYMENT CONFIGURATION ============
# Private key for deployment (DO NOT commit this to git)
PRIVATE_KEY=your_private_key_here

# Optional: BaseScan API key for contract verification
BASESCAN_API_KEY=your_basescan_api_key_here

# ============ BACKEND CONFIGURATION ============
# RPC URL for Base network
RPC_URL=https://sepolia.base.org

# Deployed contract address (updated with new deployment)
CONTRACT_ADDRESS=0x19CF09A38Bd71BC5e7D5bFAd17EBd6A0269F2be9

# Private key for backend operations (owner functions)
DEFAULT_PRIVATE_KEY=your_backend_private_key_here

# Database path for SQLite
DATABASE_PATH=./swearjar.db

# Server port
PORT=8080

# ============ FRONTEND CONFIGURATION ============
# API endpoints
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_WS_URL=ws://localhost:8080/ws

# Contract and network info
NEXT_PUBLIC_CONTRACT_ADDRESS=0x19CF09A38Bd71BC5e7D5bFAd17EBd6A0269F2be9
NEXT_PUBLIC_RPC_URL=https://sepolia.base.org

# ============ FARCASTER INTEGRATION ============
# Neynar API key for Farcaster data
NEYNAR_API_KEY=your_neynar_api_key_here

# Farcaster API endpoints
FARCASTER_API_URL=https://api.neynar.com/v2/farcaster

# ============ MINI APP CONFIGURATION ============
# Base Mini App manifest URL
NEXT_PUBLIC_MANIFEST_URL=https://your-domain.vercel.app/.well-known/farcaster.json

# Mini App splash and icon URLs
NEXT_PUBLIC_SPLASH_IMAGE_URL=https://your-domain.vercel.app/splash.png
NEXT_PUBLIC_ICON_URL=https://your-domain.vercel.app/icon.png
